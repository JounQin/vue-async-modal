{
  "name": "vue-async-modal",
  "version": "1.0.0",
  "description": "Flexible modal component for Vue with ability of asynchronous lazy loading",
  "main": "dist/vue-async-modal.js",
  "module": "lib/index.js",
  "repository": "https://github.com/JounQin/vue-async-modal.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch webpack.config.babel.js --exec \"webpack-dev-server --hot\"",
    "doc": "rimraf docs && cross-env NODE_ENV=production webpack --progress -p && git add docs",
    "lint": "eslint . --ext js --max-warnings 10",
    "lint:fix": "npm run lint -- --fix",
    "build": "rollup -c",
    "prod": "cross-env NODE_ENV=production npm run build",
    "dist": "npm run build && npm run prod",
    "release": "npm run dist && npm run doc"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap-styl": "^5.0.8",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-1stg": "^1.0.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.0",
    "pug": "^2.0.0-rc.3",
    "rimraf": "^2.6.1",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-vue": "^2.4.1",
    "rollup-watch": "^4.3.1",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
